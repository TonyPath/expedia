<?php

namespace Entities;

use Doctrine\ORM\Mapping as ORM;

/**
 * RegionHotelMapping
 */
class RegionHotelMapping
{
    /**
     * @var integer
     */
    private $regionID;

    /**
     * @var integer
     */
    private $eanHotelID;

    /**
     * @var \Entities\Hotel
     */
    private $hotel;

    /**
     * @var \Entities\Region
     */
    private $region;


    /**
     * Set regionID
     *
     * @param integer $regionID
     * @return RegionHotelMapping
     */
    public function setRegionID($regionID)
    {
        $this->regionID = $regionID;
    
        return $this;
    }

    /**
     * Get regionID
     *
     * @return integer 
     */
    public function getRegionID()
    {
        return $this->regionID;
    }

    /**
     * Set eanHotelID
     *
     * @param integer $eanHotelID
     * @return RegionHotelMapping
     */
    public function setEanHotelID($eanHotelID)
    {
        $this->eanHotelID = $eanHotelID;
    
        return $this;
    }

    /**
     * Get eanHotelID
     *
     * @return integer 
     */
    public function getEanHotelID()
    {
        return $this->eanHotelID;
    }

    /**
     * Set hotel
     *
     * @param \Entities\Hotel $hotel
     * @return RegionHotelMapping
     */
    public function setHotel(\Entities\Hotel $hotel = null)
    {
        $this->hotel = $hotel;
    
        return $this;
    }

    /**
     * Get hotel
     *
     * @return \Entities\Hotel 
     */
    public function getHotel()
    {
        return $this->hotel;
    }

    /**
     * Set region
     *
     * @param \Entities\Region $region
     * @return RegionHotelMapping
     */
    public function setRegion(\Entities\Region $region = null)
    {
        $this->region = $region;
    
        return $this;
    }

    /**
     * Get region
     *
     * @return \Entities\Region 
     */
    public function getRegion()
    {
        return $this->region;
    }
}